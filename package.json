{
  "name": "ml-talk-demo",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "dependencies": {
    "@tensorflow-models/body-pix": "^2.2.0",
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow/tfjs-backend-cpu": "^3.6.0",
    "@tensorflow/tfjs-backend-webgl": "^3.6.0",
    "@tensorflow/tfjs-converter": "^3.6.0",
    "@tensorflow/tfjs-core": "^3.6.0",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "reveal.js": "^4.1.1",
    "stats.js": "^0.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@types/stats.js": "^0.17.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.27.0",
    "eslint-config-google": "^0.14.0",
    "parcel-bundler": "^1.12.5",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "dev": "parcel index.html demo1.html demo2.html talk.html --no-hmr --open",
    "build": "parcel build index.html demo1.html demo2.html talk.html",
    "start": "http-server -a localhost .",
    "lint": "eslint *.ts",
    "typecheck": "tsc --noEmit",
    "certbot": "certbot certonly --manual -d ml-demo.philiplehmann.ch --work-dir certbot/work --config-dir certbot/config --logs-dir certbot/logs --preferred-challenges dns"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "google",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "rules": {
      "require-jsdoc": 0,
      "@typescript-eslint/ban-ts-comment": 0
    }
  },
  "eslintIgnore": [
    "dist/"
  ]
}